@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section Style
{
    <link href="/Content/jquery-ui/jquery-ui.min.css" rel="stylesheet" />
    @Styles.Render("~/styles/subpage")
    @RenderSection("style", false)
}
@section scriptHeader
{
    <script type="text/javascript">
        var pageId = "@ViewBag.PageId", returnValue = @Html.Raw(ViewBag.returnValue??"null");
    </script>
    @RenderSection("scriptHeader", false)
}
@RenderBody()
@section scriptFooter
{
    <script src="/Scripts/jquery-ui/jquery-ui.min.js"></script>
    <script src="/Scripts/jquery-ui/jquery.ui.datepicker-zh-CN.min.js"></script>
    @Scripts.Render("~/scripts/subpage")
    @RenderSection("scriptFooter", false)
}